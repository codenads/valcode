function shellShort(array: int[], n: int): void {
    var gap: int = n // 2;

    while gap > 0 {
        var i: int = gap;
        
        for i, 1, n {
            var temp: int = array[i];
            var j: int = i;

            while j >= gap and arr[j-gap] > temp {
                array[j] = array[j-gap];
                j = j - gap;
            }
            array[j] = temp;
        }

        gap = gap // 2;
    }
}

function init(): int {
    var array: int[];
    var n: int;
    input(n);
    var i: int;

    for i = 0, 1, n {
        input(array[i]);
    }

    for i = 0, 1, n {
        print("%d ", array[i]);
    }

    shellShort(array, n);

    for i = 0, 1, n {
        print("%d ", array[i]);
    }
    return 0;
}